{
  "name": "t-react",
  "version": "0.0.4",
  "description": "A react hook for easier implementation of t",
  "main": "./dist/index.cjs",
  "exports": {
    "basic": "./dist/index.js",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup src/index.ts --config rollup.config.js",
    "test:lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "prepack": "npm run build",
    "prettier": "prettier --write 'src/**/*.{js,ts}'",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage && if [[ $OSTYPE == darwin* ]]; then open ./coverage/lcov-report/index.html; else xdg-open ./coverage/lcov-report/index.html; fi",
    "test:types": "tsc",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.bus.zalan.do:ephemerals/t-react.git"
  },
  "contributors": [
    {
      "name": "Sadie Braun",
      "email": "sadie@knoblau.ch"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "t": "0.0.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-typescript": "^8.2.3",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "commitlint": "^13.1.0",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^7.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.0",
    "rollup-plugin-dts": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --ext .js,.jsx,.ts,.tsx"
    ]
  }
}
